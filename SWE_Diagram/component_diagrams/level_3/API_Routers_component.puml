  BorderColor #555
}
skinparam rectangle {
  BackgroundColor #E8F5E9
  BorderColor #555
}

component "API Routers" as ApiRoutersComp {
}

rectangle "AdminRouter" as AdminRouter
rectangle "AuthRouter" as AuthRouter
rectangle "CakeRouter" as CakeRouter
rectangle "CheckoutRouter" as CheckoutRouter
rectangle "DiscountRouter" as DiscountRouter
